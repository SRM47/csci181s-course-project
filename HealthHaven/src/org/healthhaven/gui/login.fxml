<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.healthhaven.controller.LoginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="404.0" prefWidth="644.0" VBox.vgrow="ALWAYS">
      <children>
            <Rectangle arcWidth="5.0" fill="#ebf5ff" height="171.0" layoutX="81.0" layoutY="138.0" stroke="BLACK" strokeMiterLimit="0.0" strokeType="INSIDE" strokeWidth="0.0" width="267.0" />
        <Label alignment="CENTER" layoutX="128.0" layoutY="29.0" style="&#10;" text="HealthHaven" textAlignment="CENTER" wrapText="false">
          <font>
            <Font name="Inter Black" size="59.0" />
          </font>
        </Label>
            <PasswordField fx:id="passwordTextfield" layoutX="134.0" layoutY="229.0" promptText="Password" />
            <Label fx:id="errorMessage" layoutX="201.0" layoutY="185.0" prefHeight="11.0" prefWidth="20.0" textFill="#e40c16">
               <font>
                  <Font size="9.0" />
               </font>
            </Label>
            <TextField fx:id="emailTextfield" layoutX="134.0" layoutY="198.0" promptText="Email" />
            <Button fx:id="submitButton" layoutX="187.0" layoutY="265.0" mnemonicParsing="false" onAction="#handleSubmit" text="Submit" />
            <Label alignment="CENTER" layoutX="182.0" layoutY="155.0" style="&#10;" text="Log In" textAlignment="CENTER" wrapText="false">
               <font>
                  <Font name="Inter Light" size="23.0" />
               </font>
            </Label>
            <Rectangle arcWidth="5.0" fill="#f4edff" height="171.0" layoutX="369.0" layoutY="138.0" stroke="BLACK" strokeMiterLimit="0.0" strokeType="INSIDE" strokeWidth="0.0" width="190.0" />
            <Label alignment="CENTER" layoutX="384.0" layoutY="163.0" prefHeight="97.0" prefWidth="161.0" style="&#10;" text="Don't Have An Account?" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Inter Light" size="23.0" />
               </font>
            </Label>
            <Button fx:id="accountCreationbutton" layoutX="409.0" layoutY="250.0" mnemonicParsing="false" onAction="#loadAccountCreationPage" text="Create One Here" />
            <Label layoutX="128.0" layoutY="92.0" prefHeight="17.0" prefWidth="222.0" text="version 1.0 | Secure and Encrypted" />
      </children>
    </AnchorPane>
  </children>
</VBox>
